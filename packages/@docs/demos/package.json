    "@mantine/hooks": "workspace: *",
    "@mantine/notifications": "workspace: *",
    "@mantine/code-highlight": "workspace: *",
    "@mantine/carousel": "workspace: *",
    "@mantine/colors-generator": "workspace: *",
    "@mantine/dates": "workspace: *",
    "@mantine/dropzone": "workspace: *",
    "@mantine/spotlight": "workspace: *",
    "@mantine/nprogress": "workspace: *",
    "@mantine/store": "workspace: *",
    "@mantine/tiptap": "workspace: *"
